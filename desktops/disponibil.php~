		// Display Server Time
			window.setInterval(function(){
			var today = new Date(),
			h = ("0"+today.getHours()).slice(-2);
			m = ("0"+today.getMinutes()).slice(-2);
			s = ("0"+today.getSeconds()).slice(-2);
			document.getElementById("server-time").innerHTML = h + ":" + m + ":" + s;
